<%= simple_form_for(@contacto) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nombre_contacto %>
    <%= f.input :telefono %>
    <%= f.input :celular %>
    <%= f.input :correo %>
    <%= f.association :cliente %>
    <%= f.input :estado %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
