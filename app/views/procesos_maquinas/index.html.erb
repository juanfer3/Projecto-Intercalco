<p id="notice"><%= notice %></p>

<h1>Procesos Maquinas</h1>

<table>
  <thead>
    <tr>
      <th>Orden produccion</th>
      <th>Contenedor de maquinas</th>
      <th>Cerrado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @procesos_maquinas.each do |proceso_maquinas| %>
      <tr>
        <td><%= proceso_maquinas.orden_produccion %></td>
        <td><%= proceso_maquinas.contenedor_de_maquinas %></td>
        <td><%= proceso_maquinas.cerrado %></td>
        <td><%= link_to 'Show', proceso_maquinas %></td>
        <td><%= link_to 'Edit', edit_proceso_maquinas_path(proceso_maquinas) %></td>
        <td><%= link_to 'Destroy', proceso_maquinas, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proceso Maquinas', new_proceso_maquinas_path %>
