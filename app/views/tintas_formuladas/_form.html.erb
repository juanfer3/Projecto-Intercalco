<%= simple_form_for(@tinta_formulada) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :linea_de_color %>
    <%= f.association :malla %>
    <%= f.input :codigo %>
    <%= f.input :descripcion %>
    <%= f.input :pantone %>
    <%= f.input :cantidad_total %>
    <%= f.input :estado %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
